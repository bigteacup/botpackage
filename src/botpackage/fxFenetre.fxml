<ScrollPane fx:id="rootScrollpane"  fitToHeight="true" fitToWidth="true" prefHeight="550" prefWidth="800"  xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <content  >
      <BorderPane fx:id="borderPane" styleClass="compteStyle1">
         <top>
            <GridPane styleClass="">
               <columnConstraints>
                  <ColumnConstraints hgrow="NEVER" minWidth="50.0" />
                  <ColumnConstraints hgrow="NEVER" minWidth="100.0" prefWidth="150.0" />
                  <ColumnConstraints hgrow="SOMETIMES" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <VBox styleClass="b">
                     <children>
                        <Button fx:id="boutonOn" styleClass="onc" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#boutonOn" text="on" />
                        <Button fx:id="boutonOff" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#boutonOff" text="off">
                           <effect>
                              <Glow />
                           </effect></Button>
                        <Button fx:id="bypassPause" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#bypassPause" text="Passer pause" />
                     </children>
                  </VBox>
                 
                  <FlowPane fx:id="lesCheckBoxs" columnHalignment="CENTER" hgap="0.0" rowValignment="TOP" styleClass="lesCheckBoxs" GridPane.columnIndex="2">
                     <children>
                        <CheckBox fx:id="casePillage" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#casePillage" selected="true" text="Auto Pillage">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                        <CheckBox fx:id="caseFetes" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#caseFetes" selected="true" text="Auto Fetes">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                        <CheckBox fx:id="caseConstruireBatiment" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#caseConstruireBatiment" selected="true" text="construire bat">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                        <CheckBox fx:id="caseEvacuerRessGrosVillages" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#caseEvacuerRessGrosVillages" selected="true" text="EvacuerRessGrosVillages">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                        <CheckBox fx:id="caseApproPetitsVillages" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#caseApproPetitsVillages" selected="true" text="Appro petits villges">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                        <CheckBox fx:id="caseNpc" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#caseNpc" selected="true" text="Auto NPC">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                        <CheckBox fx:id="caseOptimiserProdHero" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#caseOptimiserProdHero" selected="true" text="OptiProd Hero">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                        <CheckBox fx:id="caseRotationVillage" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#caseRotationVillage" selected="true" text="RotationVillage">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                            <CheckBox fx:id="caseMonterChamps" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#caseMonterChamps" selected="true" text="MonterChamps">                          
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                        <CheckBox fx:id="caseMonterChampCapitale" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#caseMonterChampCapitale" selected="true" text="MonterChampsCapitale">                          
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                        <CheckBox  fx:id="caseFaireAventures" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#caseFaireAventures" selected="true" text="Aventures">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                             <CheckBox fx:id="caseAcheterAuMarché" contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" onAction="#caseAcheterAuMarché"  selected="true" text="AcheterAuMarché">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                                                <CheckBox   contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false"  selected="true" text="">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                                                <CheckBox  contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false" selected="true" text="">
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                                                <CheckBox   contentDisplay="RIGHT" graphicTextGap="2.0" mnemonicParsing="false"  selected="true" text="" styleClass=""> 
                           <FlowPane.margin>
                              <Insets />
                           </FlowPane.margin>
                        </CheckBox>
                     </children>
                     <padding>
                        <Insets left="5.0" />
                     </padding>
                  </FlowPane>
               </children>
            </GridPane>
         </top>
         <center>
            <TabPane fx:id="tabbedPane" styleClass="b" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                  <Tab fx:id="comptes" styleClass="lesOnglets" closable="false" onSelectionChanged="#fxChargerComptesSwitch" text="Comptes"><!-- onSelectionChanged=" -->
                     <content>
                        <TilePane fx:id="comptesTilePane"  styleClass="compteStyle1" prefHeight="200.0" prefWidth="200.0" />
                     </content>
                  </Tab>
               
                <Tab closable="false" text="Console" styleClass="lesOnglets">
                     <content>
                        <ScrollPane fx:id="fxConsolescrollpane" fitToHeight="true" fitToWidth="true"   styleClass="compteStyle1">
                           <content>
                          <!--    <VBox fx:id="vbox" prefWidth="100.0" styleClass="consoleVBox" />  -->
                           </content>
                           </ScrollPane>
                     </content>
                  </Tab>
                
                <Tab closable="false" text="Parametres" styleClass="lesOnglets">
               		 <content>
						<ScrollPane  styleClass="compteStyle1">
						<content>
               		     	<!--   <fx:include fx:id="fxOngletParametres" source="/botpackage/fxOngletParametres.fxml"/>  -->
               		     	<!--   <fx:include fx:id="fxOngletParametres" source="fxOngletParametres.fxml"/>   	pour dev  --> 
               		     	<fx:include fx:id="fxOngletParametres" source="/botpackage/fxOngletParametres.fxml"/>
                			
                			</content>
                           </ScrollPane>

               		 </content>
                </Tab>
              
                <Tab closable="false" text="Xpath perso" styleClass="lesOnglets">
                     <content >
                        <ScrollPane fitToHeight="true" fitToWidth="true">
                           <content>
                             <ToggleButton mnemonicParsing="false" text="ToggleButton">
                           <graphic>
                              <Label text="Label" />
                           </graphic>
                        </ToggleButton>
                              <VBox  prefHeight="200.0" prefWidth="100.0"  styleClass="compteStyle1"/>
                           </content>
                        </ScrollPane>
                     </content></Tab>
                <Tab closable="false" onSelectionChanged="#peupleractifSwitch" text="Villages" styleClass="lesOnglets">
                     <content>
                        <ScrollPane fitToHeight="true" fitToWidth="true">
                           <content>
                              <VBox fx:id="villageAnchorPane" prefHeight="200.0" prefWidth="100.0"  styleClass="compteStyle1"/>
                           </content>
                        </ScrollPane>
                     </content></Tab>
                 <Tab closable="false"  onSelectionChanged="#faireOngletTemplateSwitch"  text="Template" styleClass="lesOnglets">
                     <content>
                        <ScrollPane fitToHeight="true" fitToWidth="true" >
                           <content>
                             <VBox fx:id="fxTemplate"  styleClass="compteStyle1" />
                           </content>
                        </ScrollPane>
                     </content>
                   </Tab>
                   <Tab closable="false"  onSelectionChanged="#faireOngletEnvoiTroupes"  text="Mouvements de Troupes" styleClass="lesOnglets">
                     <content>
                        <ScrollPane fitToHeight="true" fitToWidth="true" >
