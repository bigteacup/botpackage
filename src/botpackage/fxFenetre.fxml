                             <VBox fx:id="fxOngletEnvoiTroupes"  styleClass="compteStyle1" />
                           </content>
                        </ScrollPane>
                     </content>
                   </Tab>
              </tabs>
            </TabPane>
         </center>
         <bottom>
          
         </bottom>
      </BorderPane>
   </content>
